{
  "name": "rideOs Algorithm Test Kit",
  "version": "1.0",
  "description": "Test rideOs data with different algorithms",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage"
  ],
  "background": {
     "scripts": ["background.js"],
     "persistent": false
   },
   "content_security_policy": "script-src 'self' https://unpkg.com/leaflet@1.4.0/dist/leaflet.js; object-src 'self'",
   "browser_action": {
      "default_popup": "index.html",
      "default_icon": {
        "16": "images/get_started16.png",
        "32": "images/get_started32.png",
        "48": "images/get_started48.png",
        "128": "images/get_started128.png"
      }
    },
    "icons": {
     "16": "images/get_started16.png",
     "32": "images/get_started32.png",
     "48": "images/get_started48.png",
     "128": "images/get_started128.png"
   },
  "manifest_version": 2
}
